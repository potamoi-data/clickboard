{
    "name": "clickboard",
    "version": "0.0.3",
    "author": {
        "name": "Naman Nehra"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/potamoi-data/clickboard.git"
    },
    "bugs": {
        "url": "https://github.com/potamoi-data/clickboard/issues"
    },
    "homepage": "https://clickboard.potamoi.dev",
    "keywords": [
        "react",
        "charts",
        "echarts",
        "dashboard",
        "clickhouse"
    ],
    "license": "MIT",
    "main": "build/clickboard.umd.js",
    "module": "build/clickboard.es.js",
    "types": "build/index.d.ts",
    "scripts": {
        "build": "vite build && yarn build-types",
        "build-demo": "vite --config vite.demo.config.ts build",
        "build-types": "tsc --project tsconfig.declaration.json",
        "check-all": "yarn check-format && yarn lint && yarn tsc && yarn test",
        "check-format": "prettier --check .",
        "dev": "vite --config vite.demo.config.ts",
        "format": "prettier --write .",
        "lint": "yarn eslint --ignore-path .prettierignore '**/*.{js,jsx,ts,tsx}'",
        "postinstall": "patch-package",
        "pre-commit": "lint-staged",
        "prepare": "husky install",
        "preview": "vite --config vite.demo.config.ts preview",
        "release": "np",
        "test": "jest"
    },
    "dependencies": {
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.6.0",
        "@fontsource/roboto": "^4.5.1",
        "@mui/icons-material": "^5.3.0",
        "@mui/material": "^5.3.0",
        "@types/lodash-es": "^4.17.5",
        "@types/react-grid-layout": "^1.3.0",
        "@types/uuid": "^8.3.4",
        "echarts": "^5.2.2",
        "echarts-for-react": "^3.0.2",
        "lodash-es": "^4.17.21",
        "patch-package": "^6.4.7",
        "react-grid-layout": "^1.3.1",
        "use-resize-observer": "^8.0.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@babel/core": "^7.16.12",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-typescript": "^7.16.7",
        "@jest/types": "^27.4.2",
        "@types/jest": "^27.4.0",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "@vitejs/plugin-react": "^1.0.7",
        "babel-jest": "^27.4.6",
        "eslint": "^8.7.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "husky": "^7.0.4",
        "immer": "^9.0.12",
        "jest": "^27.4.7",
        "lint-staged": "^12.1.2",
        "lodash": "^4.17.21",
        "np": "^7.6.0",
        "postinstall-postinstall": "^2.1.0",
        "prettier": "^2.5.1",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "ts-node": "^10.4.0",
        "typescript": "~4.5.4",
        "use-immer": "^0.6.0",
        "vite": "^2.7.2",
        "vite-tsconfig-paths": "^3.3.17"
    },
    "peerDependencies": {
        "@types/react": "^17.0.0",
        "react": "^17.0.0"
    },
    "peerDependenciesMeta": {
        "@types/react": {
            "optional": true
        }
    },
    "np": {
        "contents": "build",
        "publish": false
    },
    "files": [
        "/build"
    ]
}
